{
  "if": {
    "allOf": [
      {
        "field": "type",
        "equals": "Microsoft.Web/sites"
      },
      {
        "field": "Microsoft.Web/sites/hostingEnvironmentProfile.type",
        "notEquals": "Microsoft.Web/hostingEnvironments"
      },
      {
        "anyOf": [
          {
            "field": "Microsoft.Web/sites/publicNetworkAccess",
            "exists": "false"
          },
          {
            "field": "Microsoft.Web/sites/publicNetworkAccess",
            "notEquals": "Disabled"
          }
        ]
      }
    ]
  },
  "then": {
    "effect": "[parameters('effect')]"
  }
}