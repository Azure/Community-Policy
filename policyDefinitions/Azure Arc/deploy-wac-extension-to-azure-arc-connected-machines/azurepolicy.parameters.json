      "allowedValues": [
        "DeployIfNotExists",
        "Disabled"
      ],
      "defaultValue": "DeployIfNotExists"
    },
    "port": {
      "type": "Integer",
      "metadata": {
        "displayName": "Port",
        "description": "Define the Port on which Windows Admin Center Extension works."
