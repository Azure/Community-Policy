{
  "if": {
    "allOf": [
      {
        "field": "type",
        "equals": "Microsoft.KeyVault/vaults"
      },
      {
        "field": "Microsoft.KeyVault/vaults/sku.name",
        "notEquals": "[parameters('allowedSKUSetting')]"
      }
    ]
  },
  "then": {
    "effect": "[parameters('effect')]"
  }
}
