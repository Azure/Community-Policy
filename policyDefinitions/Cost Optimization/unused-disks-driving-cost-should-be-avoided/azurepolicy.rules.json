{
  "if": {
    "allOf": [
      {
        "field": "type",
        "equals": "Microsoft.Compute/disks"
      },
      {
        "field": "Microsoft.Compute/disks/diskState",
        "equals": "Unattached"
      },
      {
        "allof": [
          {
            "field": "name",
            "notlike": "*-ASRReplica"
          },
          {
            "field": "name",
            "notlike": "ms-asr-*"
          },
          {
            "field": "name",
            "notlike": "asrseeddisk-*"
          }
        ]
      }
    ]
  },
  "then": {
    "effect": "[parameters('effect')]"
  }
}
