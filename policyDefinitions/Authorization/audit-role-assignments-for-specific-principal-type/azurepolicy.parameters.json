{
  "principalType": {
    "type": "String",
    "metadata": {
      "description": "Which principalType to audit against e.g. 'User, Group, ServicePrincipal'",
      "displayName": "principalType"
    },
    "allowedValues": [
      "User",
      "Group",
      "ServicePrincipal"
    ],
    "defaultValue": "User"
  },
  "effect": {
    "type": "String",
    "metadata": {
      "displayName": "Effect",
      "description": "Audit or Disabled the execution of the Policy"
    },
    "allowedValues": [
      "Audit",
      "Disabled"
    ],
    "defaultValue": "Audit"
  }
}
