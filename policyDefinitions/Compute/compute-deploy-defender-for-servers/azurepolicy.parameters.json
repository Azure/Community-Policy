{
  "AccountLocation": {
    "type": "String",
    "metadata": {
      "description": "Location where the policy should apply"
    }
  },
  "effect": {
    "type": "String",
    "metadata": {
      "displayName": "Effect",
      "description": "DeployIfNotExists, AuditIfNotExists or Disabled the execution of the Policy"
    },
    "allowedValues": [
      "DeployIfNotExists",
      "AuditIfNotExists",
      "Disabled"
    ],
    "defaultValue": "DeployIfNotExists"
  }
}
