{
  "if": {
    "allOf": [
      {
        "field": "type",
        "equals": "Microsoft.DocumentDB/databaseAccounts"
      },
      {
        "field": "Microsoft.DocumentDB/databaseAccounts/enableMultipleWriteLocations",
        "notEquals": "[parameters('multipleWriteLocationsMustBeEnabled')]"
      }
    ]
  },
  "then": {
    "effect": "[parameters('effect')]"
  }
}