{
  "effect": {
    "type": "String",
    "metadata": {
      "displayName": "Effect",
      "description": "Enable or disable the execution of the policy"
    },
    "allowedValues": [
      "AuditIfNotExists",
      "Disabled"
    ],
    "defaultValue": "AuditIfNotExists"
  },
  "retentionInDays": {
    "type": "String",
    "metadata": {
      "displayName": "Retention in Days",
      "description": "How long should deleted resources be retained for."
    },
    "minValue": 1,
    "maxValue": 365
  }
}
