{
  "if": {
    "allOf": [
      {
        "field": "type",
        "equals": "Microsoft.CognitiveServices/accounts"
      },
      {
        "not": {
          "field": "kind",
          "in": "[parameters('listOfAllowedKind')]"
        }
      }
    ]
  },
  "then": {
    "effect": "[parameters('effect')]"
  }
}
