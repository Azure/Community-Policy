{
    "autoPatchingSetting": {
      "type": "Boolean",
      "metadata": {
        "displayName": "Auto Patching Setting",
        "description": "A boolean value for whether or not Auto Patching needs to be enabled on a SQL Server VM."
      },
      "defaultValue": true
    },
    "effect": {
      "type": "String",
      "metadata": {
        "displayName": "Effect",
        "description": "Audit, Deny, or Disable Auto Patching."
      },
      "allowedValues": [
        "Audit",
        "Deny",
        "Disabled"
      ],
      "defaultValue": "Audit"
    }
}